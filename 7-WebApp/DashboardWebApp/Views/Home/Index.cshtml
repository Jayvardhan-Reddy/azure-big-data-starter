@{
    ViewBag.Title = "Home Page";
    Layout = null;
}

@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

<div class="jumbotron">
    <h3 class="col-md-2 col-md-offset-5"></h3>
    <div class="col-md-4 col-md-offset-3 line">Make: <span class="values" id="make"></span></div>
    <div class="col-md-4 col-md-offset-3 line">Device Time: <span class="values" id="deviceTime"></span></div>
    <div class="col-md-4 col-md-offset-3 line">Speed (KM/H): <span class="values" id="speed"></span></div>
    <div class="col-md-4 col-md-offset-3 line">RPM: <span class="values" id="rpm"></span></div>
</div>

<div class="row">
    <div id="map" style="height: 504px; width:1013px;" class="col-md-2 col-md-offset-2">
    </div>
</div>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyALCddOIQTqNa-RjRrAPB3YAn8h3kLxYFo&callback=initMap"
        async defer></script>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/signalr")
@Scripts.Render("~/bundles/map")
@Scripts.Render("/signalr/hubs")
@Scripts.Render("~/bundles/mapInitialize")